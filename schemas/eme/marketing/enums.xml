<?xml version="1.0" encoding="UTF-8" ?>
<pbj-enums xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/enums.xsd">

  <enums namespace="eme:marketing">
    <enum name="campaign-send-status" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="DRAFT" value="draft"/>
      <option key="SCHEDULED" value="scheduled"/>
      <option key="SENDING" value="sending"/>
      <option key="SENT" value="sent"/>
      <option key="CANCELED" value="canceled"/>
      <option key="PAUSED" value="paused"/>
      <option key="FAILED" value="failed"/>
    </enum>

    <enum name="search-campaigns-sort" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="RELEVANCE" value="relevance"/>
      <option key="CREATED_AT_ASC" value="created-at-asc"/>
      <option key="CREATED_AT_DESC" value="created-at-desc"/>
      <option key="UPDATED_AT_ASC" value="updated-at-asc"/>
      <option key="UPDATED_AT_DESC" value="updated-at-desc"/>
      <option key="SEND_AT_ASC" value="send-at-asc"/>
      <option key="SEND_AT_DESC" value="send-at-desc"/>
      <option key="SENT_AT_ASC" value="sent-at-asc"/>
      <option key="SENT_AT_DESC" value="sent-at-desc"/>
      <option key="TITLE_ASC" value="title-asc"/>
      <option key="TITLE_DESC" value="title-desc"/>
    </enum>

    <enum name="search-profiles-sort" type="string">
      <option key="UNKNOWN" value="unknown"/>
      <option key="RELEVANCE" value="relevance"/>
      <option key="CREATED_AT_DESC" value="created-at-desc"/>
      <option key="CREATED_AT_ASC" value="created-at-asc"/>
      <option key="UPDATED_AT_DESC" value="updated-at-desc"/>
      <option key="UPDATED_AT_ASC" value="updated-at-asc"/>
      <option key="FIRST_NAME_DESC" value="first-name-desc"/>
      <option key="FIRST_NAME_ASC" value="first-name-asc"/>
      <option key="LAST_NAME_DESC" value="last-name-desc"/>
      <option key="LAST_NAME_ASC" value="last-name-asc"/>
    </enum>
  </enums>
</pbj-enums>
