<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:eme:collector:mixin:collectable:1-0-0" mixin="true">
    <fields>
      <field name="collector" type="message">
        <description>
          The application collecting the message.  This is set on the
          server by the collector app itself.
        </description>
        <any-of>
          <curie>gdbots:contexts::app</curie>
        </any-of>
      </field>
    </fields>
  </schema>
</pbj-schema>
