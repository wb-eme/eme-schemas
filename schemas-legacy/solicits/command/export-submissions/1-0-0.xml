<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:eme:solicits:command:export-submissions:1-0-0">
    <fields>
      <field name="search_request" type="message" required="true">
        <any-of>
          <curie>eme:solicits:request:search-submissions-request</curie>
        </any-of>
      </field>
    </fields>

    <mixins>
      <curie-major>eme:accounts:mixin:account-ref:v1</curie-major>
      <curie-major>gdbots:pbjx:mixin:command:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
