<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:eme:solicits:command:process-irr:1-0-0">
    <fields>
      <field name="wb_subject_request_id" type="uuid"/>
      <field name="wb_status_callback_uri" type="text" format="url"/>
      <field name="wb_fulfillment_callback_uri" type="text" format="url"/>
      <field name="onetrust_subtask_id" type="uuid"/>
    </fields>

    <mixins>
      <curie-major>eme:accounts:mixin:account-ref:v1</curie-major>
      <curie-major>gdbots:pbjx:mixin:command:v1</curie-major>
      <curie-major>gdbots:forms:mixin:process-irr:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
