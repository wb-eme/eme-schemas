<?xml version="1.0" encoding="UTF-8" ?>
<pbj-schema xmlns="http://gdbots.io/pbj/xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://gdbots.io/pbj/xsd http://gdbots.io/pbj/xsd/schema.xsd">

  <schema id="pbj:eme:accounts:request:get-active-accounts-response:1-0-0">
    <fields>
      <field name="accounts" type="message" rule="map">
        <any-of>
          <curie>eme:accounts:node:account</curie>
        </any-of>
      </field>
    </fields>

    <mixins>
      <curie-major>gdbots:pbjx:mixin:response:v1</curie-major>
    </mixins>
  </schema>
</pbj-schema>
