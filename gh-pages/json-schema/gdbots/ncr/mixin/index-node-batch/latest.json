{
  "id": "http://schemas.wbeme.com/json-schema/gdbots/ncr/mixin/index-node-batch/1-0-0.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "node_refs": {
      "type": "array",
      "items": [
        {
          "type": "string",
          "pattern": "^[\\w\\/\\.:-]+$"
        }
      ],
      "uniqueItems": true,
      "minItems": 1,
      "additionalProperties": false,
      "pbj": {
        "type": "identifier",
        "rule": "set"
      }
    }
  },
  "required": [
    "node_refs"
  ],
  "additionalProperties": true
}